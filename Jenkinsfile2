pipeline{
    agent any
    stages{
        stage("build code"){
            steps{
                sh "mvn install"
                sh "ls -lrt target/"
                sh "mv target/*war target/bookstore-${BUILD_NUMBER}.war"
                sh "ls -lrt target/"
                sh "printenv"
            }
        }
        stage("test code"){
            steps{
                sh "mvn test"
            }
        }
    }
}
